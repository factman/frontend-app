{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
      "localStorage": true,
      "sessionStorage": true,
      "fetch": true,
      "document": true,
      "window": true,
      "Image": true,
      "FileReader": true,
      "isNaN": true
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": [2, "double", { "avoidEscape": true }],
        "react/prop-types": 0,
        "linebreak-style": 0,
        "no-underscore-dangle": ["error", { "allow": ["_remoteAddress", "_id","_startTime"] }],
        "no-console": 0,
        "no-unused-vars": 1,
        "no-param-reassign": 0,
        "eslint linebreak-style": [0, "error", "windows"],
        "no-tabs": 0,
        "no-array-index-key": 0,
        "no-control-regex": 0,
        "object-curly-newline": [
            "error",
            {
              "ObjectExpression": {
                "multiline": true,
                "consistent": true
              },
              "ObjectPattern": {
                "multiline": true,
                "consistent": true
              }
            }
          ]
    }
}